{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "sourceMap": false,
    "strict": false,
    "rootDir": "./src",
    "baseUrl": "./src",
    "paths": {
      "#app/*": ["*.ts"],
      "#app": ["."]
    },
    "outDir": "./build",
    "noEmit": true
  }
}
